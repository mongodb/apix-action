{
  "name": "verify-changed-files",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action to check if files have changed and output the results",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write **/*.ts",
    "all": "npm run test && npm run build"
  },
  "keywords": [
    "github",
    "actions",
    "verify",
    "files",
    "changes",
    "mongodb"
  ],
  "author": "MongoDB <info@mongodb.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.8.2",
    "@vercel/ncc": "^0.38.0",
    "globals": "^16.3.0",
    "jest": "^29.5.0",
    "ncc": "^0.3.6",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.38.0"
  },
  "engines": {
    "node": ">=20"
  }
}
